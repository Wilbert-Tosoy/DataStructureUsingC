#include <stdio.h>

int main(void) {
    int size;

    printf("Enter the size of the array: ");
    scanf("%d", &size);

    int arr[size];

    for (int i = 0; i < size; i++) {
        printf("Enter the value of index %d: ", i);
        scanf("%d", &arr[i]);
    }

    int length;
    length = ((size - 1) - 0) + 1;

    printf("Array Length: %d", length);

    return 0;
}
