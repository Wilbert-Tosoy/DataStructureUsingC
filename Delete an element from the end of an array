#include <stdio.h>

// Delete an element from the end of an array.
int main(void) {
    int i;

    int size;
    printf("Enter the array size: ");
    scanf("%d", &size);

    // Inputting the elements
    int arr[size];
    for (i = 0; i < size; i++) {
        printf("Array[%d]: ", i);
        scanf("%d", &arr[i]);
    }

    printf("Before the deletion: ");
    for (i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }

    printf("\nAfter the deletion: ");
    size = size - 1;
    for (i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
