#include <stdio.h>
#include <math.h>

int main() {
    int size;
    printf("Enter the array size: ");
    scanf("%d", &size);

    int array[size];
    for (int i = 0; i < size; i++) {
        printf("Array[%d]: ", i);
        scanf("%d", &array[i]);
    }

    int sum = 0;

    for (int i = 0; i < size; i++) {
        sum = sum + pow(array[i], 2);
    }

    printf("%d", sum);

    return 0;
}
